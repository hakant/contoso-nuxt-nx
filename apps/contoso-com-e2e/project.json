{
    "name": "contoso-com-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/contoso-com-e2e/src",
    "projectType": "application",
    "targets": {
      "e2e": {
        "executor": "@nx/cypress:cypress",
        "options": {
          "cypressConfig": "apps/contoso-com-e2e/cypress.config.ts",
          "devServerTarget": "contoso-com:dev",
          "baseUrl": "http://localhost:3000",
          "testingType": "e2e",
          "skipServe": true,
          "headed": false
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": ["{options.outputFile}"],
        "options": {
          "lintFilePatterns": ["apps/contoso-com-e2e/**/*.{js,ts}"]
        }
      }
    },
    "tags": [],
    "implicitDependencies": ["contoso-com"]
  }